// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Tarefa {
  id        String  @id @default(uuid())
  titulo    String
  descricao String
  data      String
  horaFim   String
  concluida Boolean
}
